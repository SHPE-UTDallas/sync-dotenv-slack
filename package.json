{
  "name": "sync-dotenv-slack",
  "version": "1.0.0",
  "description": "",
  "main": "dist/cli.js",
  "scripts": {
    "clean": "rimraf dist",
    "bundle": "parcel build cli.ts --target node --no-cache",
    "build": "npm-run-all --parallel clean bundle",
    "build:start": "npm run build -- start",
    "dev": "nodemon --ext ts --exec 'npm run build && node dist/cli.js'",
    "start": "node dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeshifu/sync-dotenv-slack.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeshifu/sync-dotenv-slack/issues"
  },
  "homepage": "https://github.com/codeshifu/sync-dotenv-slack#readme",
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "nodemon": "1.19.3",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.3",
    "rimraf": "3.0.0",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "@slack/web-api": "5.2.0",
    "axios": "0.19.0",
    "dotenv": "8.1.0",
    "parse-dotenv": "2.0.1"
  }
}
