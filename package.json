  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "concurrently": "4.1.2",
    "nodemon": "1.19.3",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.3",
    "rimraf": "3.0.0",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "@slack/events-api": "2.3.0",
